{
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "python.formatting.provider": "Black",
    "python.analysis.typeCheckingMode": "off",
    "githubPullRequests.ignoredPullRequestBranches": [
        "main"
    ],
    "sqltools.connections": [
        {
            "previewLimit": 100,
            "driver": "SQLite",
            "name": "epanda_dev",
            "database": "P:\\epanda_dev.db"
        },
        {
            "previewLimit": 100,
            "driver": "SQLite",
            "name": "epanda_prod",
            "database": "P:\\epanda_prod.db"
        },
        {
            "previewLimit": 50,
            "driver": "SQLite",
            "name": "test",
            "database": "${workspaceFolder:PANDA-BEAR}/test_db.db"
        },
        {
            "previewLimit": 50,
            "driver": "SQLite",
            "name": "mirror_test",
            "database": "${workspaceFolder:PANDA-BEAR}/epanda_test.db"
        },
        {
            "previewLimit": 1000,
            "driver": "SQLite",
            "database": "${workspaceFolder:PANDA-BEAR}/epanda_test.db",
            "name": "epanda_test"
        }
    ],
    "sqltools.useNodeRuntime": true,
    "editor.rulers": [80,120],

}